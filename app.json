{
  "expo": {
    "name": "E-Memory",
    "slug": "ememory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ememory"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#6366f1",
          "sounds": ["./assets/sounds/notification.wav"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#6366f1",
          "image": "./assets/images/splash.png",
          "imageWidth": 200
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.ememory"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1dd3989f-63e4-4fed-9ccf-9d2dc4686854"
      }
    },
    "owner": "lucashcr",
    "sdkVersion": "52.0.0",
    "entryPoint": ".vscode/exponentIndex.js",
    "splash": {
      "backgroundColor": "#6366f1",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    }
  },
  "name": "e-memory"
}
